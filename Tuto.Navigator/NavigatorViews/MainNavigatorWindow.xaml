<Window x:Class="Tuto.Navigator.MainNavigatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:model="clr-namespace:Tuto.Model;assembly=Tuto"
        xmlns:navigator="clr-namespace:Tuto.Navigator"
        Title="{Binding WindowTitle}" Height="350" Width="800"
        >
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <DockPanel>
        <navigator:BatchWorkPanel DataContext="{Binding Queue}" DockPanel.Dock="Bottom"/>
        <navigator:VideothequePanel/>
    </DockPanel>
</Window>
